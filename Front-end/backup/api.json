    "host": "https://lab5.live/Quiz/",
    "basePath": "/API/V1",
    "tags": [{
      "name": "Quiz API",
      "description": "Everything about quiz API",
      "externalDocs": {
        "description": "Find out more",
        "url": "http://swagger.io"
      }
    }],
    "schemes": [
      "https",
      "http"
    ],
    "paths": {
  
      "/computing": {
        "get": {
          "tags": [
            "Computing Quiz"
          ],
          "summary": "get computing questions ",
          "description": "Questions related to computing with their options and answers",
          "operationId": "computing",
          "produces": [
            "application/json"
          ],
  
          "responses": {
            "200": {
              "description": "successful operation"
            },
            "400": {
              "description": "Invalid question "
            }
          }
        }
      },
      "/personal": {
        "get": {
          "tags": [
            "Personal Quiz"
          ],
          "summary": "Get personal questions",
          "description": "Personal questions with their options and answers",
          "operationId": "question",
          "produces": [
            "application/xml",
            "application/json"
          ],
          "responses": {
            "200": {
              "description": "successful operation"
            },
            "400": {
              "description": "Invalid Question "
            },
            "404": {
              "description": "Question not found"
            }
          }
        }
      }
    }
  
  }